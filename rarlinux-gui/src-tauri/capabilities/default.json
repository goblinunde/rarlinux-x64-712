{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default permissions for RARLinux GUI application",
  "platforms": [
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-create",
    "core:window:allow-center",
    "core:window:allow-set-title",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:allow-exists",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-create",
    "fs:allow-mkdir"
  ]
}